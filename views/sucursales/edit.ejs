<div class="mdc-layout-grid">
  <div class="mdc-layout-grid__inner">
    <div class="mdc-layout-grid__cell--span-4 mdc-layout-grid__cell--span-7-desktop stretch-card">
      <div class="mdc-card p-0 to-do-list">
        <div class="d-flex bg-primary text-white to-do-list-header">
          <h4 class="card-title">Editar Sucursal</h4>
        </div>
        <div class="mdc-card">
          <form action="/sucursales/edit/<%= sucursal.cod_sucursal %>" method="POST">
            <div class="template-demo">
              <div class="mdc-layout-grid__inner">
                <div class="mdc-layout-grid__cell stretch-card mdc-layout-grid__cell--span-3-desktop">
                  <div class="mdc-text-field w-100">
                    <input class="mdc-text-field__input" id="text-field-hero-input" name="cod_sucursal"
                      value="<%= sucursal.cod_sucursal %>" />
                    <div class="mdc-line-ripple"></div>
                    <label for="text-field-hero-input" class="mdc-floating-label">Código Sucursal</label>
                  </div>
                </div>
                <div class="mdc-layout-grid__cell stretch-card mdc-layout-grid__cell--span-6-desktop">
                  <div class="mdc-text-field w-100">
                    <input class="mdc-text-field__input" id="text-field-hero-input" name="nombre"
                      value="<%= sucursal.nombre %>" />
                    <div class="mdc-line-ripple"></div>
                    <label for="text-field-hero-input" class="mdc-floating-label">Nombre</label>
                  </div>
                </div>
                <div class="mdc-layout-grid__cell stretch-card mdc-layout-grid__cell--span-3-desktop">
                  <div class="mdc-text-field w-100">
                    <input class="mdc-text-field__input" id="text-field-hero-input" name="telefono"
                      value="<%= sucursal.telefono %>" />
                    <div class="mdc-line-ripple"></div>
                    <label for="text-field-hero-input" class="mdc-floating-label">Teléfono</label>
                  </div>
                </div>
                <div class="mdc-layout-grid__cell stretch-card mdc-layout-grid__cell--span-12-desktop">
                  <div class="mdc-text-field w-100">
                    <input class="mdc-text-field__input" id="text-field-hero-input" name="direccion"
                      value="<%= sucursal.direccion %>" />
                    <div class="mdc-line-ripple"></div>
                    <label for="text-field-hero-input" class="mdc-floating-label">Dirección</label>
                  </div>
                </div>
                <div class="mdc-layout-grid__cell stretch-card mdc-layout-grid__cell--span-3-desktop">
                  <div class="mdc-text-field w-100">
                    <input class="mdc-text-field__input" id="text-field-hero-input" name="id_municipio"
                      value="<%= sucursal.id_municipio %>" />
                    <div class="mdc-line-ripple"></div>
                    <label for="text-field-hero-input" class="mdc-floating-label">Municipio</label>
                  </div>
                </div>
                <!--<div class="mdc-layout-grid__cell stretch-card mdc-layout-grid__cell--span-3-desktop">
                              <div class="mdc-text-field w-100">
                                <input class="mdc-text-field__input"  id="text-field-hero-input" name="id_municipio" value="<%= sucursal.departamento %>"/>
                                <div class="mdc-line-ripple"></div>
                                <label for="text-field-hero-input" class="mdc-floating-label">Municipio</label>
                              </div>
                          </div>-->
                <div class="mdc-layout-grid__cell stretch-card mdc-layout-grid__cell--span-2-desktop">
                  <div class="mdc-text-field w-100">
                    <input class="mdc-text-field__input" id="text-field-hero-input" name="estado"
                      value="<%= sucursal.estado %>" />
                    <div class="mdc-line-ripple"></div>
                    <label for="text-field-hero-input" class="mdc-floating-label">Estado</label>
                  </div>
                </div>
                <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-12-desktop">
                  <div class="template-demo">
                    <button type="submit" class="mdc-button mdc-button--unelevated">
                      <i class="material-icons">refresh</i>&nbsp;Guardar Cambios
                    </button>
                    <button type="reset" class="mdc-button mdc-button--unelevated filled-button--warning">
                      <i class="material-icons">cancel</i>&nbsp;Cancelar
                    </button>
                    <a class="mdc-button mdc-button--unelevated filled-button--success" href='..'><i class="material-icons">arrow_back</i>&nbsp;Volver
                    </a>
                  </div>
                </div>
              </div>
            </div>
          </form>
        </div>
      </div>
    </div>
  </div>
</div>